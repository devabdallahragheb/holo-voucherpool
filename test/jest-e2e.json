{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
"testRegex": ".*\\.(spec|e2e-spec)\\.ts$"
 ,
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "setupFiles": ["<rootDir>/test/setup.ts"],
  "moduleNameMapper": {
    "^typeorm$": "<rootDir>/node_modules/typeorm"
  }
}
